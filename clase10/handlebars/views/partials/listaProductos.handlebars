<section style="text-align: center;">
    <h2>Lista Producto</h2>
    {{#if listExist}}
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Nombre</th>
                    <th scope="col">Precio</th>
                    <th scope="col">Imagen</th>
                </tr>
            </thead>
            {{#each productos}}
                <tbody style="line-height: 50px;">
                    <td>{{this.name}}</td>
                    <td>${{this.precio}}</td>
                    <td><img style="width: 20%;" src={{this.urlImg}} alt=""></td>
                </tbody>
            {{/each}}
        </table>
    {{/if}}
    {{#else}}
        <h4>No hay productos en la lista</h4>
    {{/else}}
</section>